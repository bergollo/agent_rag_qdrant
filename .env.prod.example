PROJECT_ID=rag-stack-chatbot
GAR_HOST=us-central1-docker.pkg.dev
GAR_REPO=agent-rag
TAG=latest

NODE_ENV=development


# Frontend metadata
FRONTEND_NAME="RAG Stack Chatbot"
FRONTEND_PORT=8080
FRONTEND_HOST=0.0.0.0
FRONTEND_DOMAIN=frontend
FRONTEND_URL=http://$FRONTEND_DOMAIN:$FRONTEND_PORT


# Backend metadata
BACKEND_NAME="RAG Stack Chatbot Backend"
BACKEND_ENV=development
BACKEND_HOST=0.0.0.0
BACKEND_PORT=8080
BACKEND_DOMAIN=backend
BACKEND_URL=http://$BACKEND_DOMAIN:$BACKEND_PORT


# AI Service metadata
AI_SERVICE_NAME="AI Service Stub"
AI_SERVICE_ENV=development
AI_SERVICE_HOST=0.0.0.0
AI_SERVICE_PORT=8001
AI_SERVICE_DOMAIN=ai_service
AI_SERVICE_URL=http://$AI_SERVICE_DOMAIN:$AI_SERVICE_PORT
# Required for OpenAI chat + embeddings
OPENAI_API_KEY=...


# Redaction Gate MCP URL
REDACTION_GATE_NAME="Redaction Gate MCP"
REDACTION_GATE_TOKEN=dev-token
REDACTION_GATE_HOST=0.0.0.0
REDACTION_GATE_PORT=8080
REDACTION_GATE_DOMAIN=redaction_gate
REDACTION_GATE_URL=http://$REDACTION_GATE_DOMAIN:$REDACTION_GATE_PORT
REDACTION_POLICY_VERSION=v0
MCP_TIMEOUT_S=3.0


# Redaction DB credentials
REDACTION_DB_HOST=redaction_db
REDACTION_DB_PORT=5432
REDACTION_DB_URL=http://$REDACTION_DB_HOST:$REDACTION_DB_PORT
REDACTION_DB_USER=redaction_user
REDACTION_DB_NAME=redaction_db
REDACTION_DB_PASSWORD=...


# Vector store connection
QDRANT_HOST=qdrant
QDRANT_PORT=6333
QDRANT_URL=http://$QDRANT_HOST:$QDRANT_PORT
QDRANT_API_KEY=...